{
  "name": "create-velox-app",
  "version": "0.6.0",
  "description": "Project scaffolder for VeloxTS framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "create-velox-app": "dist/cli.js"
  },
  "files": [
    "dist",
    "src/templates/source",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "check:templates": "tsc -p tsconfig.templates.json --noEmit || true",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "smoke-test": "./scripts/smoke-test.sh"
  },
  "dependencies": {
    "@clack/prompts": "0.11.0",
    "ora": "8.1.1",
    "picocolors": "1.1.1"
  },
  "devDependencies": {
    "@types/node": "25.0.0",
    "typescript": "5.9.3"
  },
  "keywords": [
    "velox",
    "create",
    "scaffold",
    "cli",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/veloxts/velox-ts-framework",
    "directory": "packages/create"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
