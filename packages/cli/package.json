{
  "name": "@veloxts/cli",
  "version": "0.4.16",
  "description": "Developer tooling and CLI commands for VeloxTS framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "velox": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:types": "tsd",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "tsd": {
    "directory": "src/migrations/__type-tests__",
    "compilerOptions": {
      "paths": {
        "@veloxts/cli": [
          "dist"
        ]
      }
    }
  },
  "dependencies": {
    "@clack/prompts": "0.11.0",
    "@veloxts/auth": "workspace:*",
    "@veloxts/core": "workspace:*",
    "@veloxts/orm": "workspace:*",
    "@veloxts/router": "workspace:*",
    "@veloxts/validation": "workspace:*",
    "commander": "14.0.2",
    "hot-hook": "0.4.0",
    "picocolors": "1.1.1",
    "pluralize": "8.0.0",
    "tsx": "4.21.0"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0"
  },
  "devDependencies": {
    "@prisma/client": "6.9.0",
    "@types/node": "25.0.0",
    "@types/pluralize": "0.0.33",
    "tsd": "0.33.0",
    "typescript": "5.9.3",
    "vitest": "4.0.15"
  },
  "keywords": [
    "velox",
    "cli",
    "dev",
    "migrate",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/veloxts/velox-ts-framework",
    "directory": "packages/cli"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
