{
  "name": "@veloxts/web",
  "version": "0.6.4",
  "description": "React Server Components integration for VeloxTS framework using Vinxi",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./actions": {
      "types": "./dist/actions/index.d.ts",
      "import": "./dist/actions/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js"
    },
    "./rendering": {
      "types": "./dist/rendering/index.d.ts",
      "import": "./dist/rendering/index.js"
    },
    "./routing": {
      "types": "./dist/routing/index.d.ts",
      "import": "./dist/routing/index.js"
    }
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@vinxi/server-functions": "0.5.1",
    "radix3": "1.1.2",
    "vinxi": "0.5.10",
    "zod": "3.25.76"
  },
  "peerDependencies": {
    "@veloxts/auth": "workspace:*",
    "@veloxts/client": "workspace:*",
    "@veloxts/core": "workspace:*",
    "@veloxts/router": "workspace:*",
    "react": ">=19.2.0",
    "react-dom": ">=19.2.0"
  },
  "peerDependenciesMeta": {
    "@veloxts/auth": {
      "optional": true
    },
    "@veloxts/client": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jsdom": "27.0.0",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@veloxts/auth": "workspace:*",
    "@veloxts/client": "workspace:*",
    "@veloxts/core": "workspace:*",
    "@veloxts/router": "workspace:*",
    "@vitest/coverage-v8": "4.0.16",
    "fastify": "5.6.2",
    "jsdom": "27.3.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-server-dom-webpack": "19.2.3",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "keywords": [
    "velox",
    "veloxts",
    "react",
    "rsc",
    "server-components",
    "vinxi",
    "typescript",
    "framework",
    "full-stack"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/veloxts/velox-ts-framework",
    "directory": "packages/web"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
