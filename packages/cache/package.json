{
  "name": "@veloxts/cache",
  "version": "0.6.80",
  "description": "Multi-driver caching layer for VeloxTS framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    },
    "./drivers/memory": {
      "types": "./dist/drivers/memory.d.ts",
      "import": "./dist/drivers/memory.js"
    },
    "./drivers/redis": {
      "types": "./dist/drivers/redis.d.ts",
      "import": "./dist/drivers/redis.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "GUIDE.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@veloxts/core": "workspace:*",
    "fastify": "5.6.2",
    "fastify-plugin": "5.1.0",
    "lru-cache": "11.1.0",
    "superjson": "2.2.2"
  },
  "peerDependencies": {
    "ioredis": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "25.0.3",
    "@veloxts/testing": "workspace:*",
    "@vitest/coverage-v8": "4.0.16",
    "ioredis": "5.6.1",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "keywords": [
    "velox",
    "cache",
    "caching",
    "redis",
    "lru",
    "memory",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/veloxts/velox-ts-framework",
    "directory": "packages/cache"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
